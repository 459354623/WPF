<UserControl
    x:Class="LayuiFundamentalElement.Views.Expander"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Lay="clr-namespace:LayuiTemplate.Control;assembly=LayuiTemplate"
    xmlns:prism="http://prismlibrary.com/"
    prism:ViewModelLocator.AutoWireViewModel="True">
    <Lay:LayExpanderPanel IsAutoFold="true">
        <Lay:LayExpander IsExpanded="true">
            <Lay:LayExpander.Header>
                <TextBlock Text="123" />
            </Lay:LayExpander.Header>
            <Lay:LayButton Height="50" />
        </Lay:LayExpander>
        <Lay:LayExpander Header="123">
            <StackPanel>
                <Lay:LayButton Height="50" />
                <Lay:LayButton Height="50" />
                <Lay:LayButton Height="50" />
                <Lay:LayButton Height="50" />
            </StackPanel>
        </Lay:LayExpander>
    </Lay:LayExpanderPanel>
</UserControl>
