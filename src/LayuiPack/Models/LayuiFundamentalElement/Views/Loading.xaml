<UserControl
    x:Class="LayuiFundamentalElement.Views.Loading"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Lay="clr-namespace:LayuiTemplate.Controls;assembly=LayuiTemplate"
    xmlns:prism="http://prismlibrary.com/"
    prism:ViewModelLocator.AutoWireViewModel="True">
    <UniformGrid Columns="2" Rows="2">
        <Lay:LayLoading IsActive="true" Type="Google" />
        <Lay:LayLoading IsActive="true" Type="Normal" />
        <Lay:LayLoading IsActive="true" Type="Simple" />
        <Lay:LayLoading IsActive="true" Type="Beat" />
    </UniformGrid>
</UserControl>
